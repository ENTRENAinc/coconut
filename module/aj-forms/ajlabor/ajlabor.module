 	$_13_2 = $form_state['values']['13_2'];
 	$_14 = $form_state['values']['14'];
 	$_14_1 = $form_state['values']['14_1'];
 	

	// QUESTION 4 to 14 --------------------------------------------------------------
	if($_1 === 'Sí') {
	  if (empty($_4_3)) {
	    form_error($form['laboral']['4_3'], t('1.1. ¿Cuál curso realizaste?: Campo requerido'));
	  }
	  
	 	if (empty($_4)) {
			form_error($form['laboral']['4'], t('2. ¿Actualmente tienes un trabajo en el que te paguen?: Campo requerido'));
		} 
		
